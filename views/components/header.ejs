<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Popken Web Solutions | Websites, webshops op maat</title>

  <link rel="stylesheet" href="css/output.css" />
</head>

<header id="navbar-1" class="bg-transparent absolute w-full z-20">
  <nav
    class="mx-auto flex max-w-7xl items-center p-4 lg:py-0 justify-between lg:px-8"
    aria-label="Global"
  >
    <div class="flex lg:flex-1">
      <a href="/" class="-m-1.5 p-1.5">
        <span class="sr-only">Popken Web Solutions</span>
        <img
          id="logo"
          style="filter: brightness(0) invert(1)"
          class="h-12 w-auto"
          src="images/logo.png"
          alt=""
        />
      </a>
    </div>
    <div class="flex lg:hidden">
      <button
        type="button"
        id="open-mobile-nav"
        class="-m-2.5 inline-flex items-center justify-center rounded-md p-2.5 text-white"
      >
        <span class="sr-only">Open menu</span>
        <svg
          class="h-6 w-6"
          fill="none"
          viewBox="0 0 24 24"
          stroke-width="1.5"
          stroke="currentColor"
          aria-hidden="true"
        >
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
            d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5"
          />
        </svg>
      </button>
    </div>
    <div id="navbarElement" class="hidden lg:flex lg:gap-x-12 text-white">
      <a
        href="/"
        class="text-sm font-semibold leading-6 hover:text-red-500 py-8"
        >Home</a
      >
      <div class="dropdown relative">
        <button
          id="dropdownNavbarLink"
          data-dropdown-toggle="dropdownNavbar"
          class="flex items-center text-sm font-semibold leading-6 hover:text-red-700 py-8"
        >
          Websites
          <svg
            class="w-5 h-5 ml-1"
            aria-hidden="true"
            fill="currentColor"
            viewBox="0 0 20 20"
            xmlns="http://www.w3.org/2000/svg"
          >
            <path
              fill-rule="evenodd"
              d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z"
              clip-rule="evenodd"
            ></path>
          </svg>
        </button>
        <!-- Dropdown menu -->
        <div
          id="dropdownNavbar"
          class="overflow-hidden z-10 absolute font-normal bg-white divide-y divide-gray-100 rounded-b-md shadow w-44 border"
        >
          <ul
            class="text-sm text-gray-700"
            aria-labelledby="dropdownLargeButton"
          >
            <li>
              <a href="/website" class="block px-4 py-2 hover:bg-gray-200"
                >Website laten maken</a
              >
            </li>
            <li>
              <a href="/webshop" class="block px-4 py-2 hover:bg-gray-200"
                >Webshop laten maken</a
              >
            </li>
            <li>
              <a href="/webbeheer" class="block px-4 py-2 hover:bg-gray-200"
                >Website beheer</a
              >
            </li>
            <li>
              <a href="/faq" class="block px-4 py-2 hover:bg-gray-200">
                Veelgestelde vragen
              </a>
            </li>
          </ul>
        </div>
      </div>
      <a
        href="/about"
        class="text-sm font-semibold leading-6 hover:text-red-500 py-8"
        >Over Ons</a
      >
      <a
        href="/contact"
        class="text-sm font-semibold leading-6 hover:text-red-500 py-8"
        >Contact</a
      >
    </div>
    <div class="hidden lg:flex lg:flex-1 lg:justify-end">
      <a
        href="/contact"
        class="text-sm font-semibold leading-6 bg-red-500 rounded-md py-1.5 px-4 text-white hover:bg-red-700"
        >Offerte aanvragen</a
      >
    </div>
  </nav>
  <!-- Mobile menu, show/hide based on menu open state. -->
  <div id="mobile-nav" class="lg:hidden hidden" role="dialog" aria-modal="true">
    <!-- Background backdrop, show/hide based on slide-over state. -->
    <div class="fixed inset-0 z-10"></div>
    <div
      class="fixed inset-y-0 right-0 z-10 w-full overflow-y-auto bg-white px-6 py-6 sm:max-w-sm sm:ring-1 sm:ring-gray-900/10"
    >
      <div class="flex items-center justify-between">
        <a href="#" class="-m-1.5 p-1.5">
          <span class="sr-only">Popken Web Solutions</span>
          <img class="h-12 w-auto" src="images/logo.png" alt="" />
        </a>
        <button
          id="close-mobile-nav"
          type="button"
          class="-m-2.5 rounded-md p-2.5 text-gray-700"
        >
          <span class="sr-only">Sluit menu</span>
          <svg
            class="h-6 w-6"
            fill="none"
            viewBox="0 0 24 24"
            stroke-width="1.5"
            stroke="currentColor"
            aria-hidden="true"
          >
            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              d="M6 18L18 6M6 6l12 12"
            />
          </svg>
        </button>
      </div>
      <div class="mt-6 flow-root">
        <div class="-my-6 divide-y divide-gray-500/10">
          <div class="space-y-2 py-6">
            <a
              href="#"
              class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50"
              >Home</a
            >
            <div class="-mx-3">
              <button
                id="mobile-dropdown-btn"
                type="button"
                class="flex w-full items-center justify-between rounded-lg py-2 pl-3 pr-3.5 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50"
                aria-controls="disclosure-1"
                aria-expanded="false"
              >
                Websites

                <svg
                  id="mobileDropdownIcon"
                  class="h-5 w-5 flex-none rotate-90 transition-all duration-300"
                  viewBox="0 0 20 20"
                  fill="currentColor"
                  aria-hidden="true"
                >
                  <path
                    fill-rule="evenodd"
                    d="M5.23 7.21a.75.75 0 011.06.02L10 11.168l3.71-3.938a.75.75 0 111.08 1.04l-4.25 4.5a.75.75 0 01-1.08 0l-4.25-4.5a.75.75 0 01.02-1.06z"
                    clip-rule="evenodd"
                  />
                </svg>
              </button>
              <!-- 'Product' sub-menu, show/hide based on menu state. -->
              <div class="mt-2 space-y-2 hidden" id="disclosure-1">
                <a
                  href="website.html"
                  class="block rounded-lg py-2 pl-6 pr-3 text-md font-medium leading-7 text-gray-900 hover:bg-gray-50"
                  >Website laten maken</a
                >
                <a
                  href="webshop.html"
                  class="block rounded-lg py-2 pl-6 pr-3 text-md font-medium leading-7 text-gray-900 hover:bg-gray-50"
                  >Webshop laten maken</a
                >
                <a
                  href="management.html"
                  class="block rounded-lg py-2 pl-6 pr-3 text-md font-medium leading-7 text-gray-900 hover:bg-gray-50"
                  >Website beheer</a
                >
                <a
                  href="faq.html"
                  class="block rounded-lg py-2 pl-6 pr-3 text-md font-medium leading-7 text-gray-900 hover:bg-gray-50"
                  >Veelgestelde Vragen</a
                >
              </div>
            </div>
            <a
              href="about.html"
              class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50"
              >Over Ons</a
            >
            <a
              href="contact.html"
              class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50"
              >Contact</a
            >
          </div>
          <div class="py-6">
            <a
              href="#"
              class="-mx-3 block rounded-lg px-3 py-2.5 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50"
              >Log in</a
            >
          </div>
        </div>
      </div>
    </div>
  </div>
</header>
